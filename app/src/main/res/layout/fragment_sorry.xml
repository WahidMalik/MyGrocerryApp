<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".SorryFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Result for : idekfns"
        android:layout_margin="@dimen/_10sdp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="We 're Sorry"
        android:textSize="@dimen/_20ssp"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/_130sdp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="There are no results for your search items"
        android:textSize="@dimen/_12ssp"
        android:layout_gravity="center"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Try different Keywords"
        android:textSize="@dimen/_12ssp"
        android:textStyle="bold"
        android:layout_gravity="center"
        android:background="@drawable/transparentbutton"
        android:layout_marginTop="@dimen/_15sdp"
        android:textColor="#E17F00"
        android:padding="@dimen/_10sdp"
        />








</LinearLayout>